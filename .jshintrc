{
  "esnext": true,
  "bitwise": false,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "freeze": true,
  "immed": true,
  "indent": 2,
  "latedef": true,
  "maxdepth": 3,
  "maxstatements": 40,
  "maxlen": 140,
  "nonew": true,
  "newcap": true,
  "noarg": true,
  "quotmark": "single",
  "undef": true,
  "unused": true,
  "strict": true,
  "trailing": true,
  "smarttabs": true,
  "globals": {
    "angular": false,
    "_": false,
    "moment": false
  },
  "validthis": true,
  "laxcomma": true,
  "sub": true,
  "maxerr": 10
}
